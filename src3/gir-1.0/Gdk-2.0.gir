          <type name="none" c:type="void"/>
        </return-value>
        <parameters>
          <parameter name="region" transfer-ownership="none">
            <doc xml:whitespace="preserve">region you intend to draw to</doc>
            <type name="Region" c:type="GdkRegion*"/>
          </parameter>
        </parameters>
      </method>
