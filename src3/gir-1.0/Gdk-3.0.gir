        <type name="gint" c:type="gint"/>
      </field>
      <field name="min_height" writable="1">
        <type name="gint" c:type="gint"/>
      </field>
      <field name="max_width" writable="1">
        <type name="gint" c:type="gint"/>
      </field>
      <field name="max_height" writable="1">
        <type name="gint" c:type="gint"/>
